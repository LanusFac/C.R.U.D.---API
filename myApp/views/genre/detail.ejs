<!DOCTYPE html>
<html>
    <head>

    </head>
    <body>
        <h1><%= genre.name %> </h1>
        <% if (genre.movies) { %>
            <h3>Movies:</h3>
            <ul>
                <% genre.movies.map(movie => { %>
                    <li>
                        <a href="/movies/<%= movie.id %> "><%= movie.title %></a> 
                    </li>
                <% }) %> 
            </ul>
        <% } %>
    </body>
</html>